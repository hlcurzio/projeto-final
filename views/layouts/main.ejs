<body is="dmx-app" id="main">



    <dmx-serverconnect id="scLogout" url="/api/auth/user_logout" noload></dmx-serverconnect>
    <dmx-serverconnect id="scUserLoggedin" url="/api/auth/user_loggedin"></dmx-serverconnect>

    <dmx-notifications id="mainNotifications" offset-y="70">

    </dmx-notifications>
    <div is="dmx-browser" id="mainBrowser"></div>
    <header class="bg-light h-auto">
        <div class="container-fluid">
            <div class="row">
                <div class="col">
                    <nav class="navbar navbar-expand-lg navbar-light bg-light">
                        <a class="navbar-brand" dmx-on:click.prevent="mainBrowser.goto('/',true,'Página inicial')">
                            <img src="/assets/unb_logo.svg" width="50" class="me-2">TCC UnB</a>
                        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbar1_collapse" aria-controls="navbar1_collapse" aria-expanded="false" aria-label="Toggle navigation">
                            <span class="navbar-toggler-icon"></span>
                        </button>
                        <div class="collapse navbar-collapse justify-content-end" id="navbar1_collapse">
                            <div class="navbar-nav">
                                <a class="nav-item nav-link" dmx-on:click.prevent="mainBrowser.goto('/',true,'Página inicial')">Página inicial</a>
                                <div class="nav-item dropdown" dmx-show="scUserLoggedin.data.identity"><a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" id="dropdown2" role="button" aria-haspopup="true" aria-expanded="false">Locais</a>
                                    <div class="dropdown-menu" aria-labelledby="dropdown2">
                                        <a class="dropdown-item" dmx-on:click.prevent="mainBrowser.goto('/locais',true,'Locais')" href="/locais" internal="true">Todos os locais</a><a class="dropdown-item" href="/categorias-de-ferramentas" internal="true">Tipos</a>

                                    </div>
                                </div>
                                <div class="nav-item dropdown" dmx-show="scUserLoggedin.data.identity"><a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" id="dropdown3" role="button" aria-haspopup="true" aria-expanded="false">Cuidados</a>
                                    <div class="dropdown-menu" aria-labelledby="dropdown3">
                                        <a class="dropdown-item" dmx-on:click.prevent="mainBrowser.goto('/ferramentas',true,'Ferramentas')" href="/cuidados" internal="true">Todas os cuidados</a><a class="dropdown-item" href="/categorias-de-ferramentas" internal="true">Públicos-alvo</a>

                                    </div>
                                </div>


                                <a class="nav-item nav-link" dmx-on:click.prevent="mainBrowser.goto('/chat',true,'Chat')" href="/chat" internal="true">Chat</a>
                                <a class="nav-item nav-link" dmx-on:click.prevent="mainBrowser.goto('/sobre',true,'Sobre')" href="/sobre" internal="true">Sobre</a>
                                <div class="nav-item dropdown" dmx-show="scUserLoggedin.data.query.admin"><a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" id="dropdown1" role="button" aria-haspopup="true" aria-expanded="false">Admin</a>
                                    <div class="dropdown-menu" aria-labelledby="dropdown3">
                                        <a class="dropdown-item" dmx-on:click.prevent="mainBrowser.goto('/admin/usuarios',true,'Admin | Usuários')" href="/admin/usuarios" internal="true">Usuários</a><a class="dropdown-item" href="/admin/disciplinas" internal="true" dmx-on:click.prevent="mainBrowser.goto('/admin/disciplinas',true,'Admin | Disciplinas')">Disciplinas</a>

                                    </div>
                                </div>
                                <div class="nav-item dropdown" dmx-show="scUserLoggedin.data.identity"><a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" id="dropdown4" role="button" aria-haspopup="true" aria-expanded="false">{{scUserLoggedin.data.query.firstname}}</a>
                                    <div class="dropdown-menu" aria-labelledby="dropdown4">
                                        <a class="dropdown-item" href="/perfil" internal="true" dmx-on:click.prevent="mainBrowser.goto('/perfil',true,'Perfil')">Perfil</a><a class="dropdown-item" dmx-on:click.prevent="modalConfirmacaoLogout.show()" href="/entrar">Sair</a>


                                    </div>
                                </div>

                            </div>
                            <button id="btnLogin" class="btn btn-primary" dmx-on:click="mainBrowser.goto('/entrar',true,'Entrar')" dmx-hide="scUserLoggedin.data.identity">Entrar</button>
                        </div>
                    </nav>
                </div>
            </div>
        </div>
    </header>
    <main class="ms-0">

        <div is="dmx-view" id="content">
            <%- await include(content, locals); %>
        </div>
    </main>
    <div class="modal" id="modalConfirmacaoLogout" is="dmx-bs5-modal" tabindex="-1">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Confirmação de logout</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                    <p>Tem certeza que deseja sair?</p>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancelar</button>
                    <button type="button" class="btn btn-danger" dmx-on:click="scLogout.load({});scUserLoggedin.reset();mainBrowser.goto('/entrar',true,'Entrar');modalConfirmacaoLogout.hide()">Sair</button>
                </div>
            </div>
        </div>
    </div>

    <script src="bootstrap/5/js/bootstrap.bundle.min.js"></script>
</body>

</html>